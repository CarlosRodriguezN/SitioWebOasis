                                            <td>Macintosh</td>
                                            <td>360</td>
                                            <td>82.78%</td>
                                            <td>87.77%</td>
                                        </tr>
                                        <tr role="row" class="even">
                                            <td class="sorting_1">Chrome</td>
                                            <td>Windows</td>
                                            <td>582</td>
                                            <td>87.24%</td>
                                            <td>90.12%</td>
                                        </tr>
                                        <tr role="row" class="odd">
                                            <td class="sorting_1">Chrome</td>
                                            <td>Linux</td>
                                            <td>172</td>
                                            <td>45.21%</td>
                                            <td>48.81%</td>
                                        </tr>
                                        <tr role="row" class="even">
                                            <td class="sorting_1">Chrome</td>
                                            <td>iOS</td>
                                            <td>86</td>
                                            <td>35.23%</td>
                                            <td>44.21%</td>
                                        </tr>
                                        <tr role="row" class="odd">
                                            <td class="sorting_1">Chrome</td>
                                            <td>iOS</td>
                                            <td>45</td>
                                            <td>23.21%</td>
                                            <td>34.67%</td>
                                        </tr>
                                        <tr role="row" class="even">
                                            <td class="sorting_1">Firefox</td>
                                            <td>Windows</td>
                                            <td>280</td>
                                            <td>63.12%</td>
                                            <td>89.34%</td>
                                        </tr>
                                        <tr role="row" class="odd">
                                            <td class="sorting_1">Firefox</td>
                                            <td>Android</td>
                                            <td>236</td>
                                            <td>58.02%</td>
                                            <td>76.19%</td>
                                        </tr>
                                        <tr role="row" class="even">
                                            <td class="sorting_1">Internet Explorer</td>
                                            <td>Windows</td>
                                            <td>145</td>
                                            <td>45.23%</td>
                                            <td>94.65%</td>
                                        </tr>
                                        <tr role="row" class="odd">
                                            <td class="sorting_1">Opera</td>
                                            <td>Windows</td>
                                            <td>328</td>
                                            <td>67.12%</td>
                                            <td>78.34%</td>
                                        </tr>
                                        <tr role="row" class="even">
                                            <td class="sorting_1">Opera</td>
                                            <td>Macintosh</td>
                                            <td>22</td>
                                            <td>87.21%</td>
                                            <td>79.81%</td>
                                        </tr>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @* CODIGO DE LA ASIGNATURA A GESTIONAR *@
    @Html.Hidden("codAsignatura", @Model.strCodAsignatura)

    @* NIVEL DE LA ASIGNATURA *@
    @Html.Hidden("nivel", @Model.strCodNivel)

    @* PARALELO *@
    @Html.Hidden("paralelo", @Model.strCodParalelo)

    @* DATOS EVALUACION ACUMULATIVA *@
    @*@Html.Hidden("dtaJsonEvAcumulativa", @Model.evAcumulativaModel.jsonEvAcumulativa.ToString());*@

    @Html.Hidden("dtaJsonEvAcumulativa", @Model.evAcumulativaModel.jsonEvAcumulativa.ToString())

    @* PARCIAL ACTIVO *@
    @Html.Hidden("dtaParcialActivo", @Model.parcialActivo.ToString())

</div>
